    doParallel,
    e1071,
    foreach,
    ggplot2,
    MASS,
    parallel,
    randomForest,
    reshape2,
    stringr,
    tree,
    rlang,
Suggests:
    gtable,
    iterators,
    klaR,
    stringi,
    knitr,
    rmarkdown,
    testthat
